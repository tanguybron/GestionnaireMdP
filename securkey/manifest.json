{
    "manifest_version": 3,
    "name": "Securkey extension",
    "description": "Password Manager",
    "version": "1",
    "icons": {
        "128": "images/128.png"
    },
    "action": {
        "default_popup": "../popup/index.html",
        "default_title": "Securkey",
        "default_icon":{
            "128":"images/128.png"
        }
    },
    "background": {
        "service_worker": "background.js"
    },
    "permissions": ["activeTab"],
    "content_scripts": [
        {
            "js": ["scripts/content.js"],
            "matches": ["<all_urls>"]
        }
    ]
}
